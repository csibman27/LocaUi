<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <Button
            android:text="Button"
            android:layout_width="156dp"
            android:layout_height="47dp"
            android:id="@+id/button"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            tools:layout_editor_absoluteY="306dp"
    />
    <TextView
            android:text="TextView"
            android:layout_width="397dp"
            android:layout_height="367dp" tools:layout_editor_absoluteY="357dp"
            tools:layout_editor_absoluteX="8dp" android:id="@+id/textView"/>
    <com.google.android.gms.maps.MapView
            android:layout_width="402dp"
            android:layout_height="215dp"
            tools:layout_editor_absoluteX="5dp" android:id="@+id/mapView" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/button"/>
    <Button
            android:text="Button"
            android:layout_width="158dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="305dp"
            android:id="@+id/button2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
    />
</android.support.constraint.ConstraintLayout>